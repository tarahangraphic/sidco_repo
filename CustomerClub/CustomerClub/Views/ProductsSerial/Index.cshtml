
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/shared/_layout.cshtml";
}
@section viewsection{
<script src="~/Script/Pages/ProductSerial.js"></script>
}
<a style="margin-bottom:10px;margin-bottom:10px;margin-right:10px;" href="~/Product/Index" class="btn btn-primary">بازگشت به صفحه محصولات</a>

<div class="panel-body">
    <div class="row animatedParent animateOnce z-index-50">
        <div class="col-lg-12 animated fadeInUp">
            <h3 class="title">سریال محصولات</h3>
        </div>
    </div>
    <div id="ProductSerialRegister">

    </div>
    <div class="row">
        <div class="col-lg-12 animatedParent animateOnce z-index-50">
            <div class="panel panel-default animated fadeInUp">
                <div class="panel-heading clearfix">
                    <h3>اطلاعات</h3>
                    <a href="~/Product/Index" class="btn btn-primary">بازگشت به صفحه محصولات</a>
                    <button style="margin-right:20px;margin-bottom:20px;" id="btnRegisterProductSerial" type="button" onclick="ProductSerialRegisterform.reset()" class="btn btn-primary" data-toggle="modal" data-target="#ProductSerialRegisterModal" data-backdrop="static">ثبت جدید</button>
                </div>
                <div class="panel-body">
                    <div id="divProductSerial" class="table-responsive">

                    </div>
                </div>
            </div>
        </div>
    </div>
